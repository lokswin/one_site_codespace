{
  "name": "OnePage Browser Container",
  "build": {
    "dockerfile": "../Dockerfile",
    "context": ".."
  },
  "runArgs": [
    "--env X11VNC_PASSWORD=${X11VNC_PASSWORD}"
  ],
  "remoteUser": "root",
  "forwardPorts": [
    5901,
    6080,
    14500
  ],
  "portsAttributes": {
    "5901": {
      "visibility": "public",
      "label": "VNC Port"
    },
    "6080": {
      "visibility": "public",
      "label": "Web Port"
    },
    "14500": {
      "visibility": "public",
      "label": "XPRA Port"
    }
  }
}
